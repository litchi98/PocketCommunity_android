<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.litchi.pocketcommunity">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/FitsSystemWindowsTheme">
        <activity android:name=".featrue.proposaldetail.ProposalDetailActivity"
            android:theme="@style/DarkStatusBarTheme"></activity>
        <activity android:name=".featrue.park.ParkActivity" />
        <activity android:name=".featrue.bill.BillActivity" />
        <activity
            android:name=".featrue.proposal.ProposalActivity"
            android:theme="@style/FitsSystemWindowsTheme"
            android:launchMode="singleTask"
            />
        <activity
            android:name=".featrue.noticedetail.NoticeDetailActivity"
            android:theme="@style/DarkStatusBarTheme" />
        <activity android:name=".featrue.home.HomeActivity"></activity>
        <activity
            android:name=".featrue.account.AccountActivity"
            android:theme="@style/DarkStatusBarTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>